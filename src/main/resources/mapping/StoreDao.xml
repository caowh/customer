<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cwh.order.customer.dao.StoreDao">

    <select id="query" parameterType="java.lang.String" resultType="cwh.order.customer.model.Store">
        SELECT openid,store_name,description,headPictureUrl,business FROM SELL_USER WHERE openid = #{openid};
    </select>

</mapper>